		function curl_server_online_check(){
			if (function_exists('curl_init')){
				@$ch = curl_init();
				curl_setopt($ch, CURLOPT_URL, "http://phpfm.sf.net");
				curl_setopt($ch, CURLOPT_RETURNTRANSFER, 1);
				curl_setopt($ch, CURLOPT_HEADER, 0);
				@curl_exec($ch);
				$errnum = curl_errno($ch);
				@curl_close($ch);
			}
			return ($errnum == "0");
